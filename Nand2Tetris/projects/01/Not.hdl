/**
 * Not gate:
 * out = not in
 */

CHIP Not {
    IN in;
    OUT out;

    PARTS:
    // Put your code here:
    // Nand(0,0) = 1
    // Nand(0,1) = Nand(1,0) = 1
    // Nand(1,1) = 0
    // Simply put Nand(in, in)
    Nand(a=in, b=in, out=out);
}